{
    "name": "Install Godot",
    "id": "godot",
    "version": "1.0.0",
    "description": "Installs a Godot Engine release",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
				"4.2-stable",
                "4.4-stable",
